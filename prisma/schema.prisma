generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender{
  male
  female
  others
}
model User{
  id Int @id @default(autoincrement())
  firstName String 
  lastName String?
  age Int
  gender Gender
}

model Preferences{
  id Int @id @default(autoincrement())
  userId Int @unique
  ageRangeMin Int 
  ageRangeMax Int 
  distanceMax Int
  interests String[]
}
